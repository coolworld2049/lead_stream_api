generator client {
  provider                    = "prisma-client-py"
  enable_experimental_decimal = true
  interface                   = asyncio
  recursive_type_depth        = 2
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id                  Int                  @id @default(autoincrement())
  type                String
  api_token               String
  product             Int
  applied_at          DateTime             @default(now())
  stream              String
  sales               Json[]
  meta                Json
  user                Json
  consent             Json
  mailing_consent     Json
  codes               Json
  passport            Json
  credit              Json
  income              Json
  addr_reg            Json
  addr_fact           Json
}
